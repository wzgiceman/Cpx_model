<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.prog.zhigangwei.cpx_model"
          xmlns:android="http://schemas.android.com/apk/res/android">


    <application
        android:name=".app.MApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".http.HttpActivity"></activity>
        <activity android:name=".permission.PermisssionActivity"></activity>
        <activity android:name=".image.ImageActivity"></activity>
        <activity android:name=".rxbus.RxBusActivity"></activity>
        <activity android:name=".easy_recyclerview.RecyclerActivity"></activity>
    </application>

</manifest>